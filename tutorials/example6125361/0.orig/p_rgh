FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101315;

boundaryField
{
    top
    {
        type            fixedValue;
        value           uniform 101315;
    }
    bottom
    {
        type            symmetry;
    }
    left
    {
        type            symmetry;
    }
    right
    {
        type            fixedValue;
        value           uniform 101315;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}


